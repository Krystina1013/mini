page {
  background: #fff;
  padding: 0 94rpx;
  font-size: 32rpx;
}

.logo {
  font-size: 38rpx;
  text-align: center;
  margin-top: 15%;
  margin-bottom: 10%;
}

.logo image {
  width: 322rpx;
  height: 390rpx;
  display: block;
  margin: auto;
}

.list {
  border-bottom: 2rpx solid rgba(153, 153, 153, .3);
  padding: 32rpx 0;
  font-size: 32rpx;
  position: relative;
  display: flex;
  align-items: center;
}

.list text {
  color: #363636;
  margin-right: 15rpx;
  margin-bottom: 3rpx;
}

.list input {
  flex-grow: 1;
}

.margin {
  margin-bottom: 20rpx;
}

.list button {
  width: 200rpx;
  height: 64rpx;
  background: rgba(75, 176, 130, .9);
  border-radius: 32rpx;
  font-size: 28rpx;
  color: #4BB082;
  padding: 0 10rpx !important;
  position: absolute;
  right: 0;
  z-index: 10;
  background: transparent;
}

.bottom{
  text-align: center;
  margin-top: 50rpx;
}
.bottom button[disabled]{
  color: #fff !important;
  background-color: rgba(75, 176, 130, .5) !important;
}
.list button[disabled] {
  color: #999 !important;
}
.list-agree {
  font-size: 22rpx;
  color: #999;
  margin-top: 40rpx;
}

.list-agree image {
  width: 54rpx;
  height: 54rpx;
  margin-right: 20rpx;
}

.login {
  width: 100%;
  height: 100rpx;
  background: #4BB082;
  box-shadow: 0px 10px 30px 0px rgba(75,176,130,0.3);
  font-size: 36rpx;
  color: #fff;
  font-weight: 600;
  text-align: center;
  border-radius: 50rpx;
  line-height: 100rpx;
  padding: 0;
}

.license navigator {
  color: #576B95;
}

.placeholder{
  color: #A4A4A4;
}
.wechat{
  justify-content: center;
  margin-top: 60rpx;
  font-size: 28rpx;
  color: #A4A4A4;
  background: transparent;
}
.wechat image{
  width: 40rpx;
  height: 32rpx;
  margin-right: 20rpx;
}