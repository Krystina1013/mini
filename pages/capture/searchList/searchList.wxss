page{
  padding-top: 30rpx;
}
.con{
  width: 100vw;
  overflow:hidden;
  padding:0 10rpx;
}
.list image{
  width: 176rpx;
  height: 176rpx;
  border-radius: 105rpx;
}
.list{
  width: 33%;
  float:left;
  padding:0 3%;
  text-align:center;
  font-size: 32rpx;
  padding-top:20rpx;
  padding-bottom:20rpx;
  border-bottom:2rpx solid #eee;
}
.layout{
  display: grid;
  grid-template-columns: 1fr 1fr 1fr;
}
.empty{
  width:100vw;
  text-align: center;
  margin-top: 15%;
}
.empty image{
  width: 380rpx;
  height: 356rpx;
}