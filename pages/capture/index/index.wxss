.layout-time{
  width: 580rpx;
  white-space: nowrap;
  overflow-x: scroll;
}
.layout-time view{
  display: inline-block;
  width: 166rpx;
  height: 56rpx;
  font-size: 28rpx;
  color: #979797;
  text-align: center;
  margin-left: 20rpx;
  line-height: 56rpx;
}
.layout-time .active{
  background: #4BB082;
  border-radius: 8rpx;
  color:#fff;
}
.bottom-content{
  padding: 0;
  margin-top: 120rpx;
}
.search-layout{
  width: 100%;
  display: grid;
  grid-template-columns:1fr 1fr 1fr;
  text-align:center;
}
.search-layout>view{
  margin-bottom: 20rpx;
}
.search-layout image{
  width: 224rpx;
  height: 232rpx;
  border-radius: 8rpx;
}
.imgs{
  width: 124rpx;
  height: 124rpx;
  position: fixed;
  right: 30rpx;
  bottom: 110rpx;
}
.empty image{
  width: 380rpx;
  height: 356rpx;
}
.empty{
  display: block;
  margin:200rpx auto 0;
  text-align: center;
  color: #979797;
  font-size: 28rpx;
}
.reset-btn{
  text-align: center;
  padding: 25rpx;
  font-size: 28rpx;
  color:#333;
}
.no-more{
  display: block;
  margin:0 auto 30rpx;
  text-align: center;
  color: #979797;
  font-size: 28rpx;
}
.icon-filter{
  width: 32rpx;
  height: 34rpx;
  vertical-align: middle;
  margin-right: 12rpx;
}
.camera-filter-con{
  width: 480rpx;
  height: 74rpx;
  margin-right: 30rpx;

}
.top-filter{
  display: flex;
}
.line-split{
  width: 2rpx;
  height: 62rpx;
  background: #f1f1f1;
  margin-right: 50rpx;
}
.van-search{
  padding:0 !important;
}
.filter-con{
  line-height: 64rpx;
  color: #333;
}
.filter-pop-con {
  width: 100%;
  background: #fff;
  font-size: 28rpx;
  color: #333;
  /* margin: 18rpx 0; */
}
.filter-pop-con .type-con{
justify-content: space-between;
align-items: center;
padding: 10rpx 32rpx;
font-size: 28rpx;
border-bottom: 2rpx solid rgba(0, 0, 0, .1);
}
view .van-dropdown-menu{
  box-shadow: none;
  width: 250rpx;
  background: transparent;
  /* border:1px solid #efefef; */
  /* border-radius: 6rpx; */
  padding-right:10rpx;
  height: 72rpx;
}
view .van-dropdown-menu__title{
  width: 240rpx;
  color: #4BB082;
  text-align: right;
}
view .van-dropdown-menu__item{
  height: 72rpx;
  line-height: 72rpx;
}
.top-month{
  justify-content: space-between;
  align-items: center;
  padding: 10rpx 32rpx;
  font-size: 28rpx;
  height: 94rpx;
  border-bottom: 2rpx solid rgba(0, 0, 0, .1);
}
.top-month image{
  width: 36rpx;
  height: 36rpx;
  margin-right: 10rpx;
}
.top-month .date{
  color: #4BB082;
}
.bottom-btn {
  justify-content: space-around;
}
.time-box{
  width: 100%;
  font-size: 28rpx;
  justify-content: space-between;
  padding: 10rpx 32rpx;
  height: 94rpx;
  line-height: 72rpx;
  border-bottom: 2rpx solid rgba(0, 0, 0, .1);
}
.time-box image{
  width: 32rpx;
  height: 32rpx;
  line-height: 72rpx;
  margin-right: 10rpx;
}
.time-box .icon{
  color: #4BB082;
}