page{
  background:#f9f9f9;
}
.top-layout{
  width: 100%;
  height: 248rpx;
  /* background: rgba(75, 176, 130, .1); */
  border-bottom: 2rpx solid rgba(0, 0, 0, .1);
  border-radius: 16rpx;
}
.top-content{
  width: 100%;
  padding:12rpx 26rpx;
  border-top:2rpx solid #f9f9f9;
  border-bottom:2rpx solid #f9f9f9;
  position: fixed;
  top:0;
  z-index: 101;
  /* height: ; */
  /* padding: 40rpx 30rpx 0; */
  background:#fff;
  /* background: rgba(75, 176, 130, .1); */
}
.top-month{
  justify-content: space-between;
  align-items: center;
  padding: 32rpx;
  font-size: 28rpx;
  border-bottom: 2rpx solid rgba(0, 0, 0, .1);
}
.top-month image{
  width: 36rpx;
  height: 36rpx;
  margin-right: 10rpx;
}
.top-month .date{
  color: #4BB082;
}
.bottom-date{
  justify-content: space-between;
  margin-top: 17rpx;
  padding: 0 10rpx;
}
.arrow{
  width: 24rpx;
  height: 48rpx;
}
.right{
  transform: rotate(180deg);
}
.days-content{
  width: 580rpx;
  overflow-x: scroll;
  font-size: 28rpx;
  color: #979797;
  line-height: 50rpx;
  white-space: nowrap;
  position: relative;
  height: 104rpx;
}
.scroll{
  position: absolute;
  top: 0;
  left:0rpx;
}
.scroll-item{
  display: inline-block;
  width: 62rpx;
  height: 104rpx;
  text-align: center;
  margin-left: 20rpx;
}
.days-content .active{
  background: #4BB082;
  color: #fff;
  border-radius: 8rpx;
}
.van-picker__confirm{
  color: #4BB082 !important;
}
